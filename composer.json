{
	"name": "nella/forms-datetime",
	"description": "Date/DateTime control for Nette Forms.",
	"homepage": "http://doc.nellafw.org/forms/datetime",
	"type": "library",
	"keywords": ["nette", "forms", "control", "datetime", "date", "input"],
	"license": "MIT",
	"authors": [
	{
		"name": "Patrik VotoÄek",
		"email": "patrik@votocek.cz",
		"homepage": "http://patrik.votocek.cz",
		"role": "Developer"
	}
	],
	"support": {
		"issues": "https://github.com/nella/forms-datetime/issues",
		"source": "https://github.com/nella/forms-datetime"
	},
	"autoload": {
		"psr-4": {
			"Nella\\Forms\\DateTime\\": "src/Nella/Forms/DateTime"
		}
	},
	"autoload-dev": {
		"classmap": ["tests/Nella"]
	},
	"require": {
		"php": ">=5.5.0",
		"nette/forms": "~2.2,<2.4.0"
	},
	"require-dev": {
		"jakub-onderka/php-console-highlighter": "~0.3.1",
		"jakub-onderka/php-parallel-lint": "~0.8.0",
		"nette/tester": "~1.3.2.1",
		"nella/coding-standard": "~2.0.0",
		"satooshi/php-coveralls": "dev-master#2fbf803"
	},
	"suggest": {
		"latte/latte": "For manual rendering support"
	},
	"archive": {
		"exclude": [
			".coveralls.yml",
			".gitattributes",
			".gitignore",
			"/.travis.yml",
			"/.travis.composer.php",
			"/build",
			"/tests"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/Vrtak-CZ/tester"
		}
	]
}
